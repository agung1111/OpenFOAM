FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.8 0 0);

boundaryField
{
    inlet-outlet
    {
        type            fixedValue;
        value           uniform (0.8 0 0);
    }

    kanat
    {
        type            noSlip;
    }

    auto1
    {
        type            empty;
    }

    auto2
    {
        type            empty;
    }
}

