FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet-outlet
    {
        type            fixedValue;
        value           uniform 1;
    }

     kanat
    {
        type            zeroGradient;
    }

    auto1
    {
        type            empty;
    }

    auto2
    {
        type            empty;
    }
}

